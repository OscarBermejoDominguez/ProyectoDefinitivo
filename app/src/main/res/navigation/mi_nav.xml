<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mi_nav"
    app:startDestination="@id/fragmentoOscar">

    <fragment
        android:id="@+id/fragmentoOscar"
        android:name="com.oscarbermejo.proyectodefinitivo.FragmentoOscar"
        android:label="fragment_fragmento_oscar"
        tools:layout="@layout/fragment_fragmento_oscar" >
        <action
            android:id="@+id/action_fragmentoOscar_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="com.oscarbermejo.proyectodefinitivo.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/mierdaPutoCom"
        android:name="com.oscarbermejo.proyectodefinitivo.MierdaPutoCom"
        android:label="fragment_mierda_puto_com"
        tools:layout="@layout/fragment_mierda_puto_com" >
        <action
            android:id="@+id/action_mierdaPutoCom_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
</navigation>